pipeline {
  //Agent mean to specify where exactly this job need to run
    agent any

    stages {

     stage('Git-clone'){
       steps {

       checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/yarli9951/Devops.git']]])
     }
     stage('Build')
     {

       steps {
         script{

           sh 'echo "This is a Maven stage"'

         }

       }
     }
     }



    }

    }
